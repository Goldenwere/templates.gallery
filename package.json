{
  "name": "@goldenwere/templates.gallery",
  "private": true,
  "author": "Goldenwere",
  "version": "1.0.0-alpha-001",
  "description": "A template for artists wanting to self-host a statically-hosted gallery displaying their work",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vue-tsc && vite build",
    "preview": "npm run build && npm run start:static",
    "start-static": "rm -rf dist/content && cp -r content dist/content && vite preview"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/marked": "^5.0.0",
    "@types/node": "^18.16.18",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "dompurify": "^3.0.3",
    "js-yaml": "^4.1.0",
    "marked": "^5.1.0",
    "marked-gfm-heading-id": "^3.0.4",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.3",
    "pug": "^3.0.2",
    "rfdc": "^1.3.0",
    "sass": "^1.63.3",
    "typescript": "^5.1.3",
    "uuid": "^9.0.0",
    "vite": "^4.3.9",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue-tippy": "^6.2.0",
    "vue-tsc": "^1.6.5"
  }
}
